#!/bin/sh
maj=0;
min=0;
for ver in `ldd -v $1/*.so | grep GLIBC_ | awk '{print $2;}' | sed 's/(GLIBC_//' | sed 's/)//' | grep -v PRIVATE`; do
    m=`echo $ver | awk -F. '{print $1;}'`
    n=`echo $ver | awk -F. '{print $2;}'`
    if test $m -gt $maj; then
        maj=$m
    fi
    if test $n -gt $min; then
        min=$n
    fi
done
echo $maj.$min
